{
  "name": "ecommerce-api",
  "version": "1.0.0",
  "description": "Projeto de ecommerce-api para a disciplina de Linguagem de programação II",
  "main": "src/main.js",
  "author": "GreenSpace",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "watch": "tsc-watch --onSuccess \"npm run alias\"",
    "alias": "tsc-alias && npm run start",
    "start": "node ./dist/main.js",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register  src/main.ts' -e ts",
    "create-module": "ts-node src/utils/create-module.ts",
    "dev": "npm run watch"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "shelljs": "^0.8.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@types/shelljs": "^0.8.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
